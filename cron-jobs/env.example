# Mintellect Cron Jobs Configuration

# API Endpoints
PLAGIARISM_API_URL=https://mintellect-bnb-plagiarism.onrender.com
MAIN_API_URL=https://api.mintellect.xyz

# Cron Schedule (every 14 minutes)
CRON_SCHEDULE="*/14 * * * *"

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/cron-jobs.log

# Health Check Endpoints (optional - will use / if not specified)
PLAGIARISM_HEALTH_ENDPOINT=/
MAIN_API_HEALTH_ENDPOINT=/

# Request Timeout (in milliseconds)
REQUEST_TIMEOUT=10000

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY=5000
